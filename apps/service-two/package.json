{
  "name": "@repo/service-two",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f ../../.env -- tsx watch --tsconfig tsconfig.build.json --clear-screen=false --inspect --import=./src/instrumentation.ts --import=@opentelemetry/instrumentation/hook.mjs --import=tsx/esm src/index.ts",
    "start": "dotenvx run -f ../../.env -- node --import=./dist/src/instrumentation.js --import=@opentelemetry/instrumentation/hook.mjs ./dist/src/index.js",
    "build": "tsc -p tsconfig.build.json",
    "check-types": "tsc -p tsconfig.build.json --noEmit"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.39.0",
    "@hono/node-server": "^1.19.5",
    "@hono/otel": "^1.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@repo/instrumentation": "workspace:*",
    "hono": "^4.10.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "tsx": "^4.19.3",
    "typescript": "5.9.3",
    "vitest": "^3.1.1"
  }
